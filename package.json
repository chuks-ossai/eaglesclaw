{
  "name": "eaglesclaw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "nodemon --inspect-brk ./bin/www",
    "migrate:refresh": "npx sequelize-cli db:migrate:undo:all",
    "migrate:all": "npx sequelize-cli db:migrate",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:refresh": "npx sequelize-cli db:seed:undo:all",
    "refresh:db": "npm run migrate:refresh && npm run migrate:all && npm run seed:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0"
  }
}
